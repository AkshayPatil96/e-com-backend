    "start": "npx tsc && env-cmd -f .env node dist/server.js",
    "dev:local": "env-cmd -f .env.local nodemon --exec ts-node --files src/server.ts",
    "dev:dev": "nodemon --exec npm run start:dev",
    "dev": "nodemon --exec npm run start",
    "ts-node": "ts-node --transpile-only --pretty src/server.ts",
    "build": "prettier --write \"**/*.ts\" && tsc",
    "build-local": "env-cmd -f .env.local tsc",
    "prettier": "prettier --check \"**/*.ts\"",
