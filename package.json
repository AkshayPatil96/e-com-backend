    "eslint": "eslint \"**/*.ts\"",
    "eslint-fix": "eslint \"**/*.ts\" --fix",
    "logs:report": "node scripts/logManager.js report",
    "logs:cleanup": "node scripts/logManager.js cleanup",
    "logs:stats": "node scripts/logManager.js stats",
    "logs:manage": "node scripts/logManager.js"
